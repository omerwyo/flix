    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tedis": "^0.1.12",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.7.4",
    "winston": "^3.2.1"
